{
  "name": "protankiserver",
  "version": "1.0.0",
  "description": "TypeScript-based server for ProTanki game",
  "main": "dist/server.js",
  "scripts": {
    "update": "ts-node scripts/generateResourceTypes.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
